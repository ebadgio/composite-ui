{
  "compilerOptions": {
      "importHelpers": true,
      "outDir": "../lib",
      "declaration": true,
      "jsx": "react",
      "target": "es5",
      "lib": [
        "dom",
        "dom.iterable",
        "es5",
        "es2015.iterable"
      ],
      "noImplicitAny": true,
      "noImplicitReturns": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "pretty": true,
      "removeComments": false,
      "module": "es2015",
      "moduleResolution": "node"
  }
}